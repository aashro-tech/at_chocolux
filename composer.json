{
    "name": "aashro/at-chocolux",
    "type": "typo3-cms-extension",
    "description": "A premium chocolate-themed website design with an elegant, modern, and responsive layout.",
    "authors": [
        {
            "name": "Aashro Tech",
            "email": "info@aashro.com",
            "role": "Developer",
            "homepage": "http://www.aashro.com/"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^13.0",
        "mask/mask": "^9.0.0",
        "aashro/at-instagram": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "AASHRO\\AtChocolux\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/at-chocolux": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "at_chocolux"
        }
    }
}